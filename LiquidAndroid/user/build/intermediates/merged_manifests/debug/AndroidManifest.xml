<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liquid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Liquid"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="com.example.liquid.Auth.CustDialog"
            android:exported="true" />
        <activity
            android:name="com.example.liquid.Appearance"
            android:parentActivityName="com.example.liquid.HomePage" />
        <activity
            android:name="com.example.liquid.DetailsBuyShare"
            android:parentActivityName="com.example.liquid.HomePage" />
        <activity android:name="com.example.liquid.HomePage" />
        <activity
            android:name="com.example.liquid.Auth.Login"
            android:parentActivityName="com.example.liquid.Auth.SignUp" />
        <activity
            android:name="com.example.liquid.Auth.SignUp"
            android:parentActivityName="com.example.liquid.Auth.FirstPage" />
        <activity android:name="com.example.liquid.Auth.FirstPage" />
        <activity android:name="com.example.liquid.Auth.Splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>